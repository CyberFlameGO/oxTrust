<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.2" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
  <rewrite pattern="/authentication/finishlogin" xmlns="http://jboss.org/schema/seam/pages"/>
  <navigation-rule>
    <from-view-id>/finishlogin.xhtml</from-view-id>
    <navigation-case>
      <from-action>#{authenticator.authenticate}</from-action>
      <to-view-id>/home.xhtml</to-view-id>
      <redirect/>
    </navigation-case>
  </navigation-rule>
</faces-config>
