<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.2" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
  <rewrite pattern="/attribute/add" xmlns="http://jboss.org/schema/seam/pages"/>
  <navigation-rule>
    <from-view-id>/attribute/addAttribute.xhtml</from-view-id>
    <navigation-case>
      <from-action>#{updateAttributeAction.add}</from-action>
      <from-outcome>success</from-outcome>
      <to-view-id>/attribute/addAttribute.xhtml</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-action>#{updateAttributeAction.add}</from-action>
      <from-outcome>no_permissions</from-outcome>
      <to-view-id>/home.xhtml</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-action>#{updateAttributeAction.add}</from-action>
      <from-outcome>failure</from-outcome>
      <to-view-id>/attribute/attributeInventory.xhtml</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-action>#{updateAttributeAction.save}</from-action>
      <from-outcome>success</from-outcome>
      <to-view-id>/attribute/attributeInventory.xhtml</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-action>#{updateAttributeAction.save}</from-action>
      <from-outcome>validation_error</from-outcome>
      <to-view-id>/attribute/addAttribute.xhtml</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-action>#{updateAttributeAction.save}</from-action>
      <from-outcome>no_permissions</from-outcome>
      <to-view-id>/home.xhtml</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-action>#{updateAttributeAction.save}</from-action>
      <from-outcome>failure</from-outcome>
      <to-view-id>/attribute/attributeInventory.xhtml</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-action>#{cc.attrs.acceptMethod}</from-action>
      <from-outcome>success</from-outcome>
      <to-view-id>/attribute/updateAttribute.xhtml</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-action>#{cc.attrs.acceptMethod}</from-action>
      <from-outcome>no_permissions</from-outcome>
      <to-view-id>/home.xhtml</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-action>#{cc.attrs.acceptMethod}</from-action>
      <from-outcome>failure</from-outcome>
      <to-view-id>/attribute/addAttribute.xhtml</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-action>#{cc.attrs.cancelMethod}</from-action>
      <from-outcome>success</from-outcome>
      <to-view-id>/attribute/updateAttribute.xhtml</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-action>#{cc.attrs.cancelMethod}</from-action>
      <from-outcome>no_permissions</from-outcome>
      <to-view-id>/home.xhtml</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-action>#{cc.attrs.cancelMethod}</from-action>
      <from-outcome>failure</from-outcome>
      <to-view-id>/attribute/addAttribute.xhtml</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-action>#{updateAttributeAction.cancel}</from-action>
      <to-view-id>/attribute/attributeInventory.xhtml</to-view-id>
      <redirect/>
    </navigation-case>
  </navigation-rule>
</faces-config>
